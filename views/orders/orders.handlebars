<div class="container-fluid" style="width:100%;min-height: 700px">
    <div class="register-logo">
        <a href="#"><b>Orders</a>
    </div>

   

    {{!-- {{# if products}} --}}
    <div style="background-color: white">
        <table style="width:100%" class="table table-hover">
            <tr>
                <th>Customers</th>
                <th>Status</th>
                <th>total</th>
                <th>Date added </th>
                <th>Date modified</th>
            </tr>
            {{# each orders}}
            <tr>
                <td>
                    <a href="/orders/orderDetails/{{this._id}}" data-toggle="tooltip" data-placement="top"
                        title="Edit" style="padding: 0%" class="btn btn-primary"><img src="https://img.icons8.com/ios/20/ffffff/visible.png"></a>
                    <a href="/products/Edit/{{this.subcategory._id}}/{{this._id}}" data-toggle="tooltip" data-placement="top"
                        title="Edit" style="padding: 0%" class="btn btn-primary">
                        <img src="https://img.icons8.com/windows/20/ffffff/edit.png">
                    </a>
                    <strong>{{this.user.profile.name}}</strong>
                </td>

                <td>
                    <strong>{{this.currentStatus}}</strong>
                </td>

                <td>
                    <strong>{{this.totalAmount}}TK</strong>
                </td>

                <td>
                   <strong>{{this.created}}</strong>
                </td>

                <td>
                    <strong>{{this.lastModified}}</strong>
                </td>
               
            </tr>
            {{/each}}
            {{!-- each product --}}
            {{!-- {{/each}} --}}
        </table>

    </div>
    {{!-- {{ else }} --}}
    {{!-- <div class="row">
        <div class="col-sm-6 col-md-6 col-md-offset-3 col-sm-offset-3">
            <h2 id="search-result" onclick="searchPartial()">No Item in found</h2>
        </div>
    </div> --}}
    {{!-- {{/if}} --}}

</div>